{
    "name": "c975l/services-bundle",
    "type": "symfony-bundle",
    "description": "Commons services used by c975L bundles",
    "keywords": ["symfony", "bundle"],
    "homepage": "https://github.com/975L/ServicesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/Laurent3170",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "cocur/slugify": "^3",
        "doctrine/orm": "^2",
        "knplabs/knp-snappy": "^1",
        "symfony/routing": "*",
        "symfony/security": "*",
        "symfony/translation": "*",
        "twig/extensions": "^1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\ServicesBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/ServicesBundle",
        "issues": "https://github.com/975L/ServicesBundle/issues"
    },
    "require-dev": {
        "rector/rector": "^0",
        "symplify/easy-coding-standard": "^5"
    }
}
